{
  "name": "markstrap",
  "version": "0.0.1",
  "authors": [
    "Paul Apostol <devel.pa@gmail.com>"
  ],
  "description": "static blogging",
  "keywords": [
    "strapdown",
    "bootstrap",
    "markdown",
    "blog"
  ],
  "license": "MIT",
  "homepage": "paul.apostol.me",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootswatch": "~3.3.4",
    "marked": "~0.3.3",
    "highlight.js": "components/highlightjs#~8.5.0",
    "mermaid": "knsv/mermaid#master",
    "strapdown": "arturadib/strapdown#gh-pages",
    "katex-build": "~0.3.0",
    "google-code-prettify": "~1.0.4",
    "strapdown-topbar": "joedf/strapdown-topbar#gh-pages"
  },
  "overrides": {
    "bootstrap": {
      "main": "x"
    },
    "strapdown": {
      "main": [
        "./src/strapdown.js",
        "./src/strapdown.css"
      ]
    },
    "highlight.js": {
      "main": [
        "highlight.pack.js"
      ]
    },
    "strapdown-topbar": {
      "main": [
        "./assets/strapdown/strapdown-topbar.js"
      ],
      "dependencies": {
        "strapdow": "*"
      }
    }
  }
}
