{
  "name": "markstrap",
  "version": "0.0.1",
  "authors": [
    "Paul Apostol <devel.pa@gmail.com>"
  ],
  "description": "static blogging",
  "keywords": [
    "strapdown",
    "bootstrap",
    "markdown",
    "blog"
  ],
  "license": "MIT",
  "homepage": "paul.apostol.me",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootswatch": "~3.3.4",
    "marked": "~0.3.3",
    "highlight.js": "components/highlightjs#~8.5.0",
    "mermaid": "knsv/mermaid#master",
    "katex-build": "~0.3.0",
    "highlightjs-line-numbers.js": "wcoder/highlightjs-line-numbers.js#~1.0.0"
  },
  "overrides": {
    "bootstrap": {
      "main": "x",
      "dependencies": {}
    },
    "highlight.js": {
      "main": [
        "highlight.pack.js"
      ]
    },
    "mermaid": {
      "main": [
        "dist/mermaid.full.js",
        "dist/mermaid.css"
      ]
    },
    "katex-build": {
      "main": [
        "katex.min.js",
        "katex.min.css",
        "contrib/auto-render.min.js"
      ]
    },
    "highlightjs-line-numbers.js": {
      "main": [
        "dist/highlightjs-line-numbers.min.js"
      ]
    }
  }
}
